class Solution:
    def postorderTraversal(self, root: TreeNode) -> list[int]:
        res = []

        def dfs(node):
            if not node:
                return
            dfs(node.left)      # Visit left subtree
            dfs(node.right)     # Visit right subtree
            res.append(node.val)  # Visit root
        
        dfs(root)
        return res
